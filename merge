1)pwd
2)cd /path/to/repository
3)git status
4)git branch
5)git branch Demo
6)git checkout Demo
7)git switch Demo
8)git branch
9)touch one_demo.txt
10)git status
11)git add one_demo.txt
12)git commit -m "Added one_demo.txt"
13)git log
14)git push origin Demo
15)git checkout main
git merge Demo
16)git push origin main
